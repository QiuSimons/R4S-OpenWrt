	option enabled '1'

config server
	option type 'tls'
	option server_group 'oversea'
	option blacklist_ip '0'
	option ip '8.8.8.8'
	option addition_arg '-exclude-default-group'
	option no_check_certificate '0'
	option enabled '1'
	option name 'Google DNS TLS'

config server
	option ip 'https://dns.containerpi.com/dns-query'
	option type 'https'
	option blacklist_ip '0'
	option server_group 'oversea'
	option no_check_certificate '0'
	option addition_arg '-exclude-default-group'
	option enabled '1'
	option name 'ContainerPI HTTPS'

config server
