	option blacklist_ip '0'
	option type 'https'
	option server_group 'oversea'
	option no_check_certificate '0'
	option enabled '1'
	option addition_arg '-exclude-default-group'
	option name 'DNS.SB HTTPS'

config server
	option enabled '1'
	option type 'https'
	option name 'doh.li HTTPS'
	option server_group 'oversea'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option ip 'https://doh.li/dns-query'
	option addition_arg '-exclude-default-group'

